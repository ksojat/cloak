(use '(cloak core tasks actions))
(import '(java.io File))

(property src-dir (File. (File. "simple") "src"))

(task
  #^{:doc "Compile project's clojure files."}
  aot #{}
  (println "Aot"))

(task
  #^{:doc "Package project's comiled classes."
     :default true}
  aot-jar #{'aot}
  (println "Aot-jar"))

(task
  #^{:doc "Remove files generated during the build."}
  clean #{}
  (println "clean"))
